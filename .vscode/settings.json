{
    "workbench.editor.customLabels.patterns": {
        "**/*.js": "${dirname(1)} - ${filename}.js"
    },
    "files.exclude": {
        "out/jsconfig.json": true
    },
    "json.schemas": [
        {
            "fileMatch": [
                "**/dependencies.json"
            ],
            "url": ".vscode/schema/dependencies.schema.json"
        }
    ],
    "chat.disableAIFeatures": true
}